};

void setup(void) {
  Serial.begin(9600);
  
  
/* Далее запускаем основной блок программы для генерации сигнала */

  dac.begin(0x60);

  Serial.println("Signal generation");

}

void loop(void) {
    uint16_t i;
 freq = analogRead(A0)+1;   // Указываем подключение потенциометра для регилирования частоты
   int per = map(freq, 0, 670, 0, 270); // Конвертируем в радианы, чтобы видеть на какой угол мы поворачиваем потенциометр
  Serial.print("Analog Reading: ");   
Serial.print(freq);                   // Выводим на печать в последовательный порт необработанные("сырые") аналоговые значения
   Serial.print("  Angle of rotation: ");     
  Serial.print(per);                   // Выводим на печать в последовательный порт значения в процентах
 Serial.println("  радиан");                 // Выводим на печать в последовательный порт символ % и новой строки
       delay(5000);                              // Регулируем задержку сигнала

  
    /* Выбираем нужное значение в зависимости от выбранного разрешения */

    #if DAC_RESOLUTION == 5
      for (i = 0; i < 32; i++)
      {
        dac.setVoltage(pgm_read_word(&(DACLookup_FullSine_5Bit[i])), false);
        delayMicroseconds(freq);
      }
    #elif DAC_RESOLUTION == 6
      for (i = 0; i < 64; i++)
      {
        dac.setVoltage(pgm_read_word(&(DACLookup_FullSine_6Bit[i])), false);
        delayMicroseconds(freq);
      }
    #elif DAC_RESOLUTION == 7
      for (i = 0; i < 128; i++)
      {
        dac.setVoltage(pgm_read_word(&(DACLookup_FullSine_7Bit[i])), false);
        delayMicroseconds(freq);
      }
    #elif DAC_RESOLUTION == 9
      for (i = 0; i < 512; i++)
      {
        dac.setVoltage(pgm_read_word(&(DACLookup_FullSine_9Bit[i])), false);
      delayMicroseconds(freq);
      }
    #elif DAC_RESOLUTION == 12
      for (i = 0; i < 4096; i++)
      {
        dac.setVoltage(pgm_read_word(&(DACLookup_FullSine_12Bit[i])), false);
        delayMicroseconds(freq);
      }
    #else    // Use 8-bit data if nothing else is specified
      for (i = 0; i < 256; i++)
      {
        dac.setVoltage(pgm_read_word(&(DACLookup_FullSine_8Bit[i])), false);
        delayMicroseconds(freq);
      }

    #endif
}

